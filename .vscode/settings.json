{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.addMissingImports": "explicit"
  },
  "prettier.tabWidth": 2,
  "prettier.useTabs": false,
  "prettier.semi": true,
  "prettier.singleQuote": false,
  "prettier.jsxSingleQuote": false,
  "prettier.trailingComma": "es5",
  "prettier.arrowParens": "always",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": [
    "advlist",
    "camelcase",
    "charmap",
    "codesample",
    "darkgradient",
    "darknone",
    "devflow",
    "downvote",
    "downvoted",
    "downvotes",
    "hasupvoted",
    "hookform",
    "insertdatetime",
    "nextjs",
    "prismjs",
    "searchreplace",
    "Svix",
    "tailwindcss",
    "tinymce",
    "trpc",
    "upvote",
    "upvoted",
    "upvotes",
    "visualblocks"
  ],
  "cSpell.enableFiletypes": ["postcss"]
}
